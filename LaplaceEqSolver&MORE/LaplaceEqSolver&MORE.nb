 RowBox[{
  RowBox[{"LaplaceEqSolver", "[", 
   RowBox[{"graph_", ",", 
    RowBox[{"OptionsPattern", "[", "]"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
