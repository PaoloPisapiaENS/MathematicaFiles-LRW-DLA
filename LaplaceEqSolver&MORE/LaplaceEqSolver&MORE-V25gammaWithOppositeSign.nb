       RowBox[{
        RowBox[{"\[Beta]", "[", "__", "]"}], "->", "1"}]}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"terms", "=", 
      RowBox[{"DeleteElements", "[", 
       RowBox[{"terms", ",", 
        RowBox[{"{", 
         RowBox[{"Zeta", "[", "3", "]"}], "}"}]}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"terms", "=", 
      RowBox[{"Outer", "[", 
       RowBox[{"List", ",", "terms"}], "]"}]}], ";", "\[IndentingNewLine]", 
