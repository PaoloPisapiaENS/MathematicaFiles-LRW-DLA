Cell[BoxData[{
 RowBox[{
  RowBox[{"Clear", "[", "AugmentedGraph", "]"}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"AugmentedGraph", "[", "graph_", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"locVertices", ",", "locEdges", ",", 
      RowBox[{"locPathStyle", "=", 
       RowBox[{"{", "}"}]}], ",", "locRoot", ",", "locSource", ",", 
      "newSource", ",", "oldSourceNeighbors", ",", "graphPlus", ",", "i"}], 
     "}"}], ",", "\[IndentingNewLine]", "\[IndentingNewLine]", 
    RowBox[{
     RowBox[{"locVertices", " ", "=", " ", 
      RowBox[{"VertexList", "@", "graph"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"locEdges", " ", "=", " ", 
      RowBox[{
       RowBox[{"EdgeList", "@", "graph"}], "/.", 
       RowBox[{"DirectedEdge", "->", "List"}]}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"locRoot", " ", "=", " ", "\[IndentingNewLine]", " ", 
      RowBox[{
       RowBox[{
        RowBox[{"Select", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"List", "@@@", 
             RowBox[{"PropertyValue", "[", 
              RowBox[{"graph", ",", "VertexLabels"}], "]"}]}], ")"}], "[", 
           RowBox[{"[", 
            RowBox[{"2", ";;"}], "]"}], "]"}], ",", 
          RowBox[{
           RowBox[{"StringMatchQ", "[", 
            RowBox[{
             RowBox[{"#", "[", 
              RowBox[{"[", "2", "]"}], "]"}], ",", 
             RowBox[{"___", "~~", "\"\<ROOT\>\""}]}], "]"}], "&"}]}], "]"}], 
        "[", 
        RowBox[{"[", 
         RowBox[{"All", ",", "1"}], "]"}], "]"}], "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"locSource", " ", "=", " ", "\[IndentingNewLine]", " ", 
      RowBox[{
       RowBox[{
        RowBox[{"Select", "[", 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"List", "@@@", 
             RowBox[{"PropertyValue", "[", 
              RowBox[{"graph", ",", "VertexLabels"}], "]"}]}], ")"}], "[", 
           RowBox[{"[", 
            RowBox[{"2", ";;"}], "]"}], "]"}], ",", 
          RowBox[{
           RowBox[{"StringMatchQ", "[", 
            RowBox[{
             RowBox[{"#", "[", 
              RowBox[{"[", "2", "]"}], "]"}], ",", 
             RowBox[{"___", "~~", "\"\<SOURCE\>\""}]}], "]"}], "&"}]}], "]"}],
         "[", 
        RowBox[{"[", 
         RowBox[{"All", ",", "1"}], "]"}], "]"}], "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"newSource", "=", 
      RowBox[{
       RowBox[{"locVertices", "[", 
        RowBox[{"[", 
         RowBox[{"-", "1"}], "]"}], "]"}], "+", "1"}]}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "Add", " ", "the", " ", "edge", " ", "from", " ", "the", " ", 
       "oldSource", " ", "to", " ", "the", " ", "newSource"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"AppendTo", "[", 
      RowBox[{"locEdges", ",", 
       RowBox[{"{", 
        RowBox[{"locSource", ",", "newSource"}], "}"}]}], "]"}], ";", 
     "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"(*", 
      RowBox[{
      "Add", " ", "all", " ", "the", " ", "edges", " ", "from", " ", "the", 
       " ", "oldSource", " ", "to", " ", "its", " ", "neighbors"}], "*)"}], 
     "\[IndentingNewLine]", 
     RowBox[{"oldSourceNeighbors", "=", 
      RowBox[{"Select", "[", 
       RowBox[{"locEdges", ",", " ", 
        RowBox[{
         RowBox[{
          RowBox[{"#", "[", 
           RowBox[{"[", "2", "]"}], "]"}], "==", "locSource"}], " ", "&"}]}], 
       "]"}]}], ";", "\[IndentingNewLine]", "\[IndentingNewLine]", 
     RowBox[{"For", "[", 
      RowBox[{
       RowBox[{"i", "=", "1"}], ",", 
       RowBox[{"i", "<=", 
        RowBox[{"Length", "[", "oldSourceNeighbors", "]"}]}], ",", 
       RowBox[{"i", "++"}], ",", "\[IndentingNewLine]", 
       RowBox[{"AppendTo", "[", 
        RowBox[{"locEdges", ",", 
         RowBox[{"{", 
          RowBox[{"locSource", ",", 
           RowBox[{"oldSourceNeighbors", "[", 
            RowBox[{"[", 
             RowBox[{"i", ",", "1"}], "]"}], "]"}]}], "}"}]}], "]"}]}], 
      "\[IndentingNewLine]", "]"}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"graphPlus", " ", "=", 
      RowBox[{
       RowBox[{"MyGraph", "[", 
        RowBox[{"locEdges", ",", "locRoot", ",", "newSource", ",", 
         RowBox[{"\"\<undirected\>\"", "->", "False"}]}], "]"}], "[", 
       RowBox[{"[", "1", "]"}], "]"}]}], ";", "\[IndentingNewLine]", 
     "\[IndentingNewLine]", 
     RowBox[{"Return", "[", 
      RowBox[{"{", 
       RowBox[{"graphPlus", ",", "locSource", ",", "newSource"}], "}"}], 
      "]"}]}]}], "\[IndentingNewLine]", "]"}]}]}], "Input",
