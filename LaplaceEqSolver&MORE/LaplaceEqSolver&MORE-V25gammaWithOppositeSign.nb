 RowBox[{
  RowBox[{"DrawFromExpression", "[", 
   RowBox[{"exp_", ",", 
    RowBox[{"OptionsPattern", "[", "]"}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"terms", ",", 
      RowBox[{"graphs", "=", 
       RowBox[{"{", "}"}]}], ",", "i"}], "}"}], ",", "\n", 
    RowBox[{
     RowBox[{"terms", "=", 
